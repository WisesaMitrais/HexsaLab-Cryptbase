<?xml version="1.0" encoding="utf-8"?>
<!-- Used in
dialog_app_info: set border in content description area.
-->
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">
    <solid
        android:color="@color/color1"/>
    <stroke
        android:width="1dp"
        android:color="@color/color4" />
    <corners
        android:radius="@dimen/card_corner_radius"/>
    <padding
        android:left="0dp"
        android:top="0dp"
        android:right="0dp"
        android:bottom="0dp" />
</shape>